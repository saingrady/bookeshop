<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jasperReport PUBLIC "-//JasperReports//DTD Report Design//EN"
 "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">
<jasperReport name="BasicReport" >
  <field name="TITLE" class="java.lang.String"/>
  <field name="AUTHOR" class="java.lang.String"/>
  <field name="PRICE" class="java.lang.Double"/>
  <pageHeader>
    <band>
    </band>
  </pageHeader>
  <columnHeader>
    <band height="20">
      <staticText>
        <reportElement x="80" y="0" width="180" height="20"/>
        <textElement>
          <font isUnderline="true"/>
        </textElement>
        <text><![CDATA[TITLE]]></text>
      </staticText>
      <staticText>
        <reportElement x="260" y="0" width="180" height="20"/>
        <textElement>
          <font isUnderline="true"/>
        </textElement>
        <text><![CDATA[AUTHOR]]></text>
      </staticText>
      <staticText>
        <reportElement x="460" y="0" width="180" height="20"/>
        <textElement>
          <font isUnderline="true"/>
        </textElement>
        <text><![CDATA[PRICE]]></text>
      </staticText>
    </band>
  </columnHeader>
  <detail>
    <band height="20">
      <textField>
        <reportElement x="80" y="0" width="180" height="20"/>
        <textFieldExpression class="java.lang.String"><![CDATA[$F{TITLE}]]></textFieldExpression>
      </textField>
      <textField pattern="0.00">
        <reportElement x="260" y="0" width="180" height="20"/>
        <textFieldExpression class="java.lang.String"><![CDATA[$F{AUTHOR}]]></textFieldExpression>
      </textField>
      <textField pattern="0.00">
        <reportElement x="460" y="0" width="180" height="20"/>
        <textFieldExpression class="java.lang.Double"><![CDATA[$F{PRICE}]]></textFieldExpression>
      </textField>
    </band>
  </detail>
  <columnFooter>
    <band>
    </band>
  </columnFooter>
  <pageFooter>
    <band height="15">
      <staticText>
        <reportElement x="0" y="0" width="40" height="15"/>
        <textElement/>
        <text><![CDATA[Page:]]></text>
      </staticText>
      <textField>
        <reportElement x="40" y="0" width="100" height="15"/>
        <textElement/>
        <textFieldExpression class="java.lang.Integer"><![CDATA[$V{PAGE_NUMBER}]]></textFieldExpression>
      </textField>
    </band>
  </pageFooter>
  <summary>
    <band>
    </band>
  </summary>
</jasperReport>
